{
  "version": 2,
  "builds": [
    {
      "src": "PokemonCard-Tracker.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "PokemonCard-Tracker.py" }
  ]
}
